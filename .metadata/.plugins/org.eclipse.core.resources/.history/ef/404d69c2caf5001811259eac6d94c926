package JavaStudy;

public class Java_Constructor {

	public static void main(String[] args) {
		
	}

}

class Calculator {
	int left;
	int right;
	
	public Calculator (int left, int right){
		this.left = left;
		this.right = right;
	}
	
	public void sum() {
		System.out.println(this.left + this.right);
	}
}