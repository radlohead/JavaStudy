package JavaStudy;

public class Java_Constructor {

	public static void main(String[] args) {
		Calculator cl = new Calculator(10, 20);
		
		cl.sum();
	}

}

class CalculatorV2 {
	int left;
	int right;
	
	public CalculatorV2 (int left, int right){
		this.left = left;
		this.right = right;
	}
	
	public void sum() {
		System.out.println(this.left + this.right);
	}
}