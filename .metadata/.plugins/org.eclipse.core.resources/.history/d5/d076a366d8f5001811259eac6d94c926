package JavaStudy;

class Java_ContructorV3 {
	int left;
	int right;
	
	public int sum(int left, int right) {
		return this.left + this.right;
	}
	
	Java_ContructorV3(){
		System.out.println(this.left);
	}
}

public class Java_ContructorV2 extends Java_ContructorV3 {
	public Java_ContructorV2() {
		this.left = 20;
	}
	
	public Java_ContructorV2(int param) {
		System.out.println(param);
	}
	
	public void Java_ContructorV4 () {
		System.out.println(this.left);
	}
	
	public static void main(String[] args) {
//		Java_ContructorV3 c1 = new Java_ContructorV3();
		Java_ContructorV2 c2 = new Java_ContructorV2();
//		System.out.println(c1.left);
		
		c2.Java_ContructorV4();
	}
}