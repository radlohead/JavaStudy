package JavaStudy;

public class Java_ContructorV2 {
	int left;
	int right;

	public Java_ContructorV2() {
		this.left = 10;
	}
	
	public Java_ContructorV2(int param) {
		System.out.println(param);
	}
	
	public int sum(int left, int right) {
		return this.left + this.right;
	}
	
	
	public static void main(String[] args) {
//		Java_ContructorV2 c1 = new Java_ContructorV2();
	}
}

class Java_ContructorV3 extends Java_ContructorV2 {
	Java_ContructorV3(){
		System.out.println(this.left);
	}
}
